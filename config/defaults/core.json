{
  "brain": {
    "default_model": "jarvis-base",
    "response_time_limit": 500,
    "concurrent_processes": 4,
    "memory": {
      "short_term_limit": "2G",
      "long_term_limit": "32G"
    }
  },
  "command": {
    "max_queue_size": 1000,
    "check_interval": 0.1,
    "default_priority": 5
  }
}
